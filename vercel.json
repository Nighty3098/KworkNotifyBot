{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/bot",
      "dest": "/api/bot.js"
    },
    {
      "src": "/cron",
      "dest": "/api/cron.js"
    }
  ],
  "crons": [
    {
      "path": "/cron",
      "schedule": "*/5 * * * *"
    }
  ]
}
